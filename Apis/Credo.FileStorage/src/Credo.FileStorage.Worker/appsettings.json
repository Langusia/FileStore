{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JCSConfig": {
    "Schedule": {
      "RunningType": "Delay",
      "Delay": "5:S",
      "StartingDate": "05:15:01",
      "DailySchedule": [ "15:24" ]
    },
    "Id": "A2249CD5-0A06-4E4F-9F01-F8EB047AC87A",
    "Name": "JobTest",
    "JCSUrl": "http://localhost:5145",
    "CheckIntervalSecond": 1
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog", "Serilog.Sinks.Seq", "Serilog.Expressions" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Hosting": "Information",
        "HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] [{TraceId}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithExceptionDetails",
      "Elapsed"
    ],
    "Properties": {
      "Application": "Credo.FileStorageApi"
    },
    "AllowedHosts": "*",
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "EndsWith(RequestPath, '/hc-lb')"
        }
      }
    ]
  }
}
